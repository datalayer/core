+ * );
+ *
+ * // Delete multiple cells
+ * await deleteCellOperation.execute(
+ *   { indices: [1, 3, 5] },
+ *   { documentId: "file:///path/to/notebook.ipynb", executor }
+ * );
+ * ```
+ */
+export declare const deleteCellOperation: ToolOperation<DeleteCellParams, DeleteCellResult>;
