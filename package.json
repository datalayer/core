    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.15",
    "globals": "^16.2.0",
    "playwright": "^1.53.2",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.15",
    "typedoc-plugin-markdown": "^4.0.0",
